<!--
  For open-source packages, the TBB20_INSTALL_DIR or TBB21_INSTALL_DIR,
  TBB22_INSTALL_DIR, or TBB30_INSTALL_DIR environment variable 
  (depending on which package is used) should be set to the folder that 
  contains the unzipped packages. If several different packages need 
  to be used simultaneously, then new TBB tags should be added for each 
  additional package (see the example below). 
  The versions name from the <TBB> tags are displayed in the TBB 
  integration menu items. 
-->
<root>
  <TBB version_name="TBB 3.0">
    <TBB_INCLUDE_DIR>$(TBB30_INSTALL_DIR)\include</TBB_INCLUDE_DIR>
    <TBB_LIB_DIR mode="32">$(TBB30_INSTALL_DIR)\lib\ia32\vc10</TBB_LIB_DIR>
    <TBB_LIB_DIR mode="64">$(TBB30_INSTALL_DIR)\lib\intel64\vc10</TBB_LIB_DIR>
    <TBB_BIN_DIR mode="32">$(TBB30_INSTALL_DIR)\bin\ia32\vc10</TBB_BIN_DIR>
    <TBB_BIN_DIR mode="64">$(TBB30_INSTALL_DIR)\bin\intel64\vc10</TBB_BIN_DIR>
  </TBB>
  <TBB version_name="TBB 2.2">
    <TBB_INCLUDE_DIR>$(TBB22_INSTALL_DIR)\include</TBB_INCLUDE_DIR>
    <TBB_LIB_DIR mode="32">$(TBB22_INSTALL_DIR)\ia32\vc10\lib</TBB_LIB_DIR>
    <TBB_LIB_DIR mode="64">$(TBB22_INSTALL_DIR)\intel64\vc10\lib</TBB_LIB_DIR>
    <TBB_BIN_DIR mode="32">$(TBB22_INSTALL_DIR)\ia32\vc10\bin</TBB_BIN_DIR>
    <TBB_BIN_DIR mode="64">$(TBB22_INSTALL_DIR)\intel64\vc10\bin</TBB_BIN_DIR>
  </TBB>
  <TBB version_name="TBB 2.1">
    <TBB_INCLUDE_DIR>$(TBB21_INSTALL_DIR)\include</TBB_INCLUDE_DIR>
    <TBB_LIB_DIR mode="32">$(TBB21_INSTALL_DIR)\ia32\vc10\lib</TBB_LIB_DIR>
    <TBB_LIB_DIR mode="64">$(TBB21_INSTALL_DIR)\em64t\vc10\lib</TBB_LIB_DIR>
    <TBB_BIN_DIR mode="32">$(TBB21_INSTALL_DIR)\ia32\vc10\bin</TBB_BIN_DIR>
    <TBB_BIN_DIR mode="64">$(TBB21_INSTALL_DIR)\em64t\vc10\bin</TBB_BIN_DIR>
  </TBB>
  <TBB version_name="TBB 2.0">
    <TBB_INCLUDE_DIR>$(TBB20_INSTALL_DIR)\include</TBB_INCLUDE_DIR>
    <TBB_LIB_DIR mode="32">$(TBB20_INSTALL_DIR)\ia32\vc10\lib</TBB_LIB_DIR>
    <TBB_LIB_DIR mode="64">$(TBB20_INSTALL_DIR)\em64t\vc10\lib</TBB_LIB_DIR>
    <TBB_BIN_DIR mode="32">$(TBB20_INSTALL_DIR)\ia32\vc10\bin</TBB_BIN_DIR>
    <TBB_BIN_DIR mode="64">$(TBB20_INSTALL_DIR)\em64t\vc10\bin</TBB_BIN_DIR>
  </TBB>
<!--
  Suppose a TBB open-source package was unzipped to D:\TBB\tbb30_018oss.
  To use this package with the plugin, you need to add the following <TBB> tag: 
  <TBB version_name="TBB 3.0.018oss">
    <TBB_INCLUDE_DIR>D:\TBB\tbb30_004oss\include</TBB_INCLUDE_DIR>
    <TBB_LIB_DIR mode="32">D:\TBB\tbb30_018oss\lib\ia32\vc10</TBB_LIB_DIR>
    <TBB_LIB_DIR mode="64">D:\TBB\tbb30_018oss\lib\intel64\vc10</TBB_LIB_DIR>
    <TBB_BIN_DIR mode="32">D:\TBB\tbb30_018oss\bin\ia32\vc10</TBB_BIN_DIR>
    <TBB_BIN_DIR mode="64">D:\TBB\tbb30_018oss\bin\intel64\vc10</TBB_BIN_DIR>
  </TBB>
-->
</root>
